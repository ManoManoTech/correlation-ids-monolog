{
    "name": "manomano-tech/correlation-ids-monolog",
    "description": "Monolog processor for manomano-tech/correlation-ids package",
    "keywords": [
        "correlation",
        "correlation-id",
        "tracing",
        "monolog",
        "processor",
        "log",
        "plugin"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "manomano-tech/correlation-ids": "^1.0",
        "monolog/monolog": "^1.0"
    },
    "require-dev": {
        "php-mock/php-mock-phpunit": "^2.1",
        "phpunit/phpunit": "^7.3",
        "symfony/debug": "^4.1",
        "symfony/phpunit-bridge": "^4.1"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ManoManoTech\\CorrelationIdMonolog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ManoManoTech\\CorrelationIdMonolog\\Tests\\": "tests/"
        }
    }
}
